{
  "name": "leonardo-color",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "generate:brands": "npm run build && node dist/scripts/generate-brand-themes.js",
    "generate:brands:prod": "npm run build && node dist/scripts/generate-brand-themes.js --env=production",
    "generate:brands:staging": "npm run build && node dist/scripts/generate-brand-themes.js --env=staging"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@adobe/leonardo-contrast-colors": "^1.0.0"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.json"
  ]
}
